{
  "manifest_version": 3,
  "name": "Groq API Extension",
  "version": "2.0",
  "description": "A powerful interface for interacting with the Groq API with advanced features",
  "permissions": ["activeTab", "storage", "clipboardWrite"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["images/*"],
      "matches": ["<all_urls>"]
    }
  ]
}