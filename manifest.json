{
    "manifest_version": 3,
    "name": "Groq Client Extension",
    "version": "1.0",
    "description": "An interface for interacting with Groq API",
    "permissions": ["activeTab", "storage"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "css": ["styles.css"]
      }
    ]
  }