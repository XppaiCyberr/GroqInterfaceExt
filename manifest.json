{
    "manifest_version": 3,
    "name": "Groq Client Extension",
    "version": "2.1",
    "description": "An interface for interacting with Groq API",
    "permissions": ["activeTab", "storage", "clipboardWrite"],
    "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["images/*"],
      "matches": ["<all_urls>"]
    }
  ]
}